{% extends 'front.html.twig' %}

{%  block body %}

    <div class="container">
        <div class="row">
            <a href="{{ path('profesionales_new') }}">Agregar Profesional</a>
            <table class="table" width="60%">
                <thead>
                <tr>
                    <th>Id</th>
                    <th>Tipoanalisis</th>
                    <th>Nombre</th>
                    <th>Valorreferencia</th>
                    <th>Actions</th>
                </tr>
                </thead>
                <tbody>
                {% for itemTipoAnalisi in itemTipoAnalises %}
                    <tr>
                        <td><a href="{{ path('itemtipoanalisis_show', { 'id': itemTipoAnalisi.id }) }}">{{ itemTipoAnalisi.id }}</a></td>
                        <td>{{ itemTipoAnalisi.tipoAnalisis }}</td>
                        <td>{{ itemTipoAnalisi.nombre }}</td>
                        <td>{{ itemTipoAnalisi.valorReferencia }}</td>
                        <td>
                            <ul>
                                <li>
                                    <a href="{{ path('itemtipoanalisis_show', { 'id': itemTipoAnalisi.id }) }}">show</a>
                                </li>
                                <li>
                                    <a href="{{ path('itemtipoanalisis_edit', { 'id': itemTipoAnalisi.id }) }}">edit</a>
                                </li>
                            </ul>
                        </td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>


        </div>
    </div>



{% endblock body %}